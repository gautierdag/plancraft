{{ bos_token }}
{% for message in messages %}
    {% if (message['role'] == 'user' or message['system'] == 'user') %}
        {{'<|user|>' + '\n' + message['content'] + '<|end|>' + '\n' + '<|assistant|>' + '\n'}}
    {% elif (message['role'] == 'assistant') %}
        {{message['content'] + '<|end|>' + '\n'}}
    {% endif %}
{% endfor %}